{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "rds",
  "projects": {
    "rds-button": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "rds/rds-button",
      "sourceRoot": "rds/rds-button/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:build",
          "options": {
            "outputPath": "dist/rds-button",
            "index": "rds/rds-button/src/index.html",
            "main": "rds/rds-button/src/main.ts",
            "polyfills": "rds/rds-button/src/polyfills.ts",
            "tsConfig": "rds/rds-button/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "rds/rds-button/src/favicon.ico",
              "rds/rds-button/src/assets"
            ],
            "styles": [
              "rds/rds-button/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "rds/rds-button/src/environments/environment.ts",
                  "with": "rds/rds-button/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "rds-button:build:production"
            },
            "development": {
              "browserTarget": "rds-button:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "rds-button:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "rds/rds-button/src/test.ts",
            "polyfills": "rds/rds-button/src/polyfills.ts",
            "tsConfig": "rds/rds-button/tsconfig.spec.json",
            "karmaConfig": "rds/rds-button/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "rds/rds-button/src/favicon.ico",
              "rds/rds-button/src/assets"
            ],
            "styles": [
              "rds/rds-button/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "rds-button"
}